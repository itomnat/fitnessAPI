{
  "version": 2,
  "functions": {
    "api/auth-login.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth-register.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth-verify.js": {
      "runtime": "nodejs18.x"
    },
    "api/workouts-get.js": {
      "runtime": "nodejs18.x"
    },
    "api/workouts-add.js": {
      "runtime": "nodejs18.x"
    },
    "api/workouts-update.js": {
      "runtime": "nodejs18.x"
    },
    "api/workouts-delete.js": {
      "runtime": "nodejs18.x"
    },
    "api/workouts-complete.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


